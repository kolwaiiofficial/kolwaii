{
	"name": "kawaii-solana-bot",
	"version": "1.0.0",
	"description": "A cute and powerful Twitter bot that integrates with the Solana blockchain",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"start": "ts-node examples/integrated-bot.ts",
		"dev": "ts-node-dev --respawn examples/integrated-bot.ts",
		"test": "mocha -r ts-node/register test/**/*.ts",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"prepare": "husky"
	},
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=8.0.0"
	},
	"keywords": [
		"solana",
		"blockchain",
		"twitter",
		"bot",
		"kawaii",
		"web3",
		"nft",
		"defi"
	],
	"author": "Your Name",
	"license": "Apache-2.0",
	"dependencies": {
		"@solana/web3.js": "^1.98.0",
		"@solana/spl-token": "^0.4.9",
		"@coral-xyz/anchor": "0.29",
		"@metaplex-foundation/mpl-token-metadata": "^3.3.0",
		"@orca-so/whirlpools-sdk": "^0.13.12",
		"@raydium-io/raydium-sdk-v2": "0.1.95-alpha",
		"@tensor-oss/tensorswap-sdk": "^4.5.0",
		"twitter-api-v2": "^1.16.1",
		"openai": "^4.77.0",
		"decimal.js": "^10.4.3",
		"dotenv": "^16.4.7",
		"bs58": "^6.0.0"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"@types/node": "^22.10.2",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"@typescript-eslint/parser": "^8.18.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.7",
		"lint-staged": "^15.3.0",
		"mocha": "^10.3.0",
		"prettier": "^3.4.2",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.7.2"
	}
}
